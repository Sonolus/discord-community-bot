{
    "compilerOptions": {
        "lib": [
            "ES2023"
        ],
        "target": "ES2022",
        "module": "Node16",
        "moduleResolution": "Node16",
        "resolveJsonModule": true,
        "strict": true,
        "skipLibCheck": true,
        "exactOptionalPropertyTypes": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./dist",
        "rootDir": "./src",
    },
}
